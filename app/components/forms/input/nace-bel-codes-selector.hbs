<PowerSelectMultiple
  @search={{perform this.loadOptions}}
  @searchEnabled={{true}}
  @selected={{this.selections}}
  @options={{this.options}}
  @onChange={{this.update}}
  @allowClear={{true}}
  @placeholder="Typ om te zoeken"
  @loadingMessage="Aan het laden..."
  @noMatchesMessage="Geen resultaten gevonden"
  as |code|>
  {{code.label}}
</PowerSelectMultiple>
